}

#health_popup_main {
    height: 1fr;
    width: 100%;
}

#health_popup_list_scroll {
    width: 46%;
    border-right: solid #2f0f0f;
    padding-right: 1;
    scrollbar-background: #160606;
    scrollbar-color: #2f0f0f;
    scrollbar-corner-color: #160606;
    scrollbar-size: 1 1;
}

#health_popup_detail_scroll {
    width: 1fr;
    padding-left: 1;
    scrollbar-background: #160606;
    scrollbar-color: #2f0f0f;
    scrollbar-corner-color: #160606;
    scrollbar-size: 1 1;
}

#health_popup_list,
#health_popup_detail {
    width: 100%;
    background: transparent;
}

#health_popup_buttons {
    width: 100%;
    border-top: solid #2f0f0f;
    align: right middle;
    padding-top: 1;
}

#health_stop_selected,
#health_retry_selected,
#close_health_popup {
    width: auto;
    min-width: 12;
    height: auto;
    background: transparent;
    color: #e8d5d5;
    border: none;
    margin: 0 1;
    padding: 0 2;
}

#health_stop_selected:hover, #health_stop_selected:focus {
    background: transparent;
    color: #ef4444;
    border: none;
}

#health_retry_selected:hover, #health_retry_selected:focus,
#close_health_popup:hover, #close_health_popup:focus {
