#chat_area_container {
    width: 1fr;
    height: 100%;
    background: #0a0202 64%;
    border-left: solid #2f0f0f;
    border-right: solid #2f0f0f;
    padding: 0;
}

#chat_area_container.-full-width {
    width: 100%;
}

#chat_history {
    height: 1fr;
    background: transparent;
    border: none;
    padding: 0;
    margin-bottom: 0;
    margin-right: 0;
    scrollbar-background: #0a0202;
    scrollbar-color: #2f0f0f;
    scrollbar-corner-color: #0a0202;
    scrollbar-size: 1 1;
}

#agent_status_display {
    height: 2;
    background: #120505 60%;
    margin: 0;
    padding: 0 2;
    border-top: solid #2f0f0f;
    border-bottom: solid #2f0f0f;
    align-vertical: middle;
}

#agent_status_display.hidden {
    display: none;
}

#status_text {
    width: 1fr;
    height: 100%;
    background: transparent;
    color: #947575;
    text-align: left;
    content-align: left middle;
    text-style: none;
    margin: 0;
    padding: 0;
}

#keymap_indicator {
    width: auto;
    height: 100%;
    background: transparent;
    color: #947575;
    text-align: right;
    content-align: right middle;
    text-style: none;
    margin: 0;
    padding: 0 0 0 1;
}

#chat_input_container {
    height: 3;
    background: #120505 64%;
    border: none;
    border-top: solid #2f0f0f;
    margin-right: 0;
    padding: 0 2;
    layout: horizontal;
    align-vertical: middle;
}

#chat_input_container:focus-within {
    border-top: solid #22d3ee;
}

#chat_input_container:focus-within #chat_prompt {
    color: #22d3ee;
    text-style: bold;
}

#chat_prompt {
    width: auto;
    height: 100%;
    padding: 0 1 0 0;
    color: #947575;
    content-align: left middle;
}

#chat_history:focus {
    border: none;
}

#chat_input {
    width: 1fr;
    height: 100%;
    background: transparent;
    border: none;
    color: #e8d5d5;
    padding: 0;
    margin: 0;
    content-align: left middle;
}

#chat_input:focus {
    border: none;
}

#chat_input .text-area--cursor-line {
    background: transparent;
}

#chat_input:focus .text-area--cursor-line {
    background: transparent;
}

#chat_input > .text-area--placeholder {
    color: #947575;
    text-style: italic;
}

#chat_input > .text-area--cursor {
    color: #22d3ee;
    background: #22d3ee;
}

.chat-placeholder {
    width: 100%;
    height: 100%;
    content-align: center middle;
    text-align: center;
    color: #947575;
    text-style: italic;
}

.chat-content {
    margin: 0 !important;
    margin-top: 0 !important;
    margin-bottom: 0 !important;
    padding: 1 2;
    background: transparent;
    width: 100%;
}

.chat-message {
    margin-bottom: 0;
    padding: 0;
    background: transparent;
    width: 100%;
}

.user-message {
    color: #f8e8e8;
    border-left: thick #22d3ee;
    padding-left: 1;
    margin-bottom: 1;
}

.tool-call {
    margin-top: 1;
    margin-bottom: 0;
    padding: 0 1;
    background: transparent;
    border: none;
    width: 100%;
}

.tool-call.status-completed {
    background: transparent;
    margin-top: 1;
    margin-bottom: 0;
}

.tool-call.status-running {
    background: transparent;
    margin-top: 1;
    margin-bottom: 0;
}

.tool-call.status-failed,
.tool-call.status-error {
    background: transparent;
    margin-top: 1;
    margin-bottom: 0;
}

.browser-tool,
.terminal-tool,
.python-tool,
.agents-graph-tool,
.file-edit-tool,
.proxy-tool,
.notes-tool,
.thinking-tool,
.web-search-tool,
.scan-info-tool,
.subagent-info-tool {
    margin-top: 1;
    margin-bottom: 0;
