Screen {
    background: #02060b;
    color: #dbe9f2;
}

#splash_screen {
    height: 100%;
    width: 100%;
    background: #02060b;
    color: #32d9ff;
    align: center middle;
    content-align: center middle;
    text-align: center;
}

#splash_content {
    width: auto;
    height: auto;
    background: transparent;
    text-align: center;
    content-align: center middle;
    padding: 1 2;
}

#main_container {
    height: 100%;
    padding: 0;
    margin: 0;
    background: #050505;
}

#content_container {
    height: 1fr;
    padding: 0;
    background: #050505;
}

#left_panel {
    width: 38;
    min-width: 30;
    height: 100%;
    background: #160606 70%;
    border-right: solid #2f0f0f;
    padding: 0;
}

#left_panel.-hidden {
    display: none;
}

#right_panel {
    width: 40;
    min-width: 30;
    height: 100%;
    background: #160606 70%;
    border-left: solid #2f0f0f;
    padding: 0;
}

#right_panel.-hidden {
    display: none;
}

#left_panel_header,
#center_panel_header,
#right_panel_header {
    height: 3;
    background: #120505 62%;
    color: #947575;
    border-bottom: solid #2f0f0f;
    padding: 0 2;
    content-align: left middle;
    text-style: bold;
}

#agents_tree {
    height: 1fr;
    background: transparent;
    border: none;
    border-title-color: #a8a29e;
    border-title-style: bold;
    padding: 0 1;
    margin-bottom: 0;
}

#stats_display {
    height: auto;
    max-height: 22;
    background: transparent;
    padding: 0 1 1 1;
    margin: 0;
}

#vulnerabilities_panel {
    height: 1fr;
    background: transparent;
    padding: 1;
    margin: 0;
    border: none;
    overflow-y: auto;
    scrollbar-background: #160606;
    scrollbar-color: #2f0f0f;
    scrollbar-corner-color: #160606;
    scrollbar-size-vertical: 1;
}

.vuln-empty {
    color: #947575;
    text-style: italic;
    content-align: center middle;
    text-align: center;
    height: 1fr;
    width: 100%;
}

.vuln-item {
    height: auto;
    width: 100%;
    padding: 1 1;
    background: transparent;
    color: #e8d5d5;
    border-left: solid #2f0f0f;
}

.vuln-item:hover {
    background: #240808;
    color: #fdf2f2;
    border-left: heavy #22d3ee;
}

VulnerabilityDetailScreen {
    align: center middle;
    background: #0a0202 84%;
}

#vuln_detail_dialog {
    grid-size: 1;
    grid-gutter: 1;
    grid-rows: 1fr auto;
    padding: 2 3;
    width: 85%;
    max-width: 110;
    height: 85%;
    max-height: 45;
    border: solid #2f0f0f;
    background: #160606;
}

#vuln_detail_scroll {
    height: 1fr;
    background: transparent;
    scrollbar-background: #160606;
    scrollbar-color: #2f0f0f;
    scrollbar-corner-color: #160606;
    scrollbar-size: 1 1;
    padding-right: 1;
}

#vuln_detail_content {
    width: 100%;
    background: transparent;
    padding: 0;
}

#vuln_detail_buttons {
    width: 100%;
    height: auto;
    align: right middle;
    padding-top: 1;
    margin: 0;
    border-top: solid #2f0f0f;
}

#copy_vuln_detail {
    width: auto;
    min-width: 12;
    height: auto;
    background: transparent;
    color: #947575;
    border: none;
    text-style: none;
    margin: 0 1;
    padding: 0 2;
}

#close_vuln_detail {
    width: auto;
    min-width: 10;
    height: auto;
    background: transparent;
    color: #e8d5d5;
    border: none;
    text-style: none;
    margin: 0;
    padding: 0 2;
}

#copy_vuln_detail:hover, #copy_vuln_detail:focus {
    background: transparent;
    color: #22d3ee;
    border: none;
}

#close_vuln_detail:hover, #close_vuln_detail:focus {
    background: transparent;
    color: #ffffff;
    border: none;
}

VulnerabilityOverlayScreen {
    align: center middle;
    background: #0a0202 84%;
}

#vuln_overlay_dialog {
    grid-size: 1;
    grid-gutter: 1;
    grid-rows: auto 1fr auto;
    padding: 2 3;
    width: 99%;
    min-width: 120;
    max-width: 220;
    height: 96%;
    min-height: 34;
    max-height: 80;
    border: solid #4a1616;
    background: #130404;
}

#vuln_overlay_header {
    height: auto;
    border: solid #3a1414;
    color: #f4f4f5;
    background: #0d0303 60%;
    padding: 1 2;
    margin-bottom: 1;
}

#vuln_overlay_main {
    height: 1fr;
    width: 100%;
}

#vuln_overlay_list_scroll {
    width: 48%;
    min-width: 52;
    border-right: solid #3a1414;
    padding-right: 1;
    margin-right: 1;
    scrollbar-background: #160606;
    scrollbar-color: #2f0f0f;
    scrollbar-corner-color: #160606;
    scrollbar-size: 1 1;
}

#vuln_overlay_detail_scroll {
    width: 1fr;
    min-width: 60;
    padding-left: 1;
    scrollbar-background: #160606;
    scrollbar-color: #2f0f0f;
    scrollbar-corner-color: #160606;
    scrollbar-size: 1 1;
}

#vuln_overlay_list,
#vuln_overlay_detail {
    width: 100%;
    background: transparent;
    color: #f8fafc;
    padding: 1 1;
}

#vuln_overlay_buttons {
    width: 100%;
    border-top: solid #3a1414;
    align: center middle;
    padding-top: 1;
}

#vuln_overlay_keyhints {
    width: 1fr;
    height: auto;
    color: #8a8a8a;
    padding: 0 1;
}

#copy_overlay_selected,
#copy_overlay_all,
#close_vuln_overlay {
    width: auto;
    min-width: 18;
    height: 3;
    background: #210c0c;
    color: #f8fafc;
    border: solid #4a1616;
    text-style: bold;
    margin: 0 1;
    padding: 0 2;
}

#copy_overlay_selected:hover, #copy_overlay_selected:focus,
#copy_overlay_all:hover, #copy_overlay_all:focus,
#close_vuln_overlay:hover, #close_vuln_overlay:focus {
    background: #2b1111;
    color: #22d3ee;
    border: solid #22d3ee;
}

AgentHealthPopupScreen {
    align: center middle;
    background: #0a0202 84%;
}

#health_popup_dialog {
